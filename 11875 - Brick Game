#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define z long long int
using namespace std ;
int main()
{
	int t,i,n,j;
	sf("%d",&t);

	for(i=1;i<=t;i++)
	{
		sf("%d",&n);
		int a[n];

		for(j=0;j<n;j++)
			sf("%d",&a[j]);
		sort(a,a+n);
if(n%2==0)
	pf("Case %d: %d\n",i, (a[n/2]+a[n/2-1])/2  );
else
	pf("Case %d: %d\n",i,a[n/2]);


	}
	
	return 0;
}
