#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define mod 1000000007
#define z long long int
using namespace std;
int main()
{
	
	z v,t;
	while(sf("%lld %lld",&v,&t)!=EOF)
		pf("%lld\n",2*v*t);


}

